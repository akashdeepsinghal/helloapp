{
    "rewrites": [
      {
        "source": "/",
        "destination": "/apps/frontend"
      },
      {
        "source": "/assets/(.*)",
        "destination": "/apps/frontend/assets/$1"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "https://helloapp-rho.vercel.app/apps/backend/:1",
        "methods": ["GET", "POST", "PUT", "PATCH", "DELETE"]
      }
    ]
  }
  