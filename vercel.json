{
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/apps/frontend/$1"
      },
      {
        "source": "/api/(.*)",
        "destination": "/apps/backend/$1"
      }
    ],
    "routes": [
      {
        "src": "/apps/frontend/(.*)",
        "dest": "/frontend/$1"
      },
      {
        "src": "/apps/backend/(.*)",
        "dest": "/backend/$1"
      }
    ],
    "version": 2
  }