{
  "version": 2,
  "functions": {
    "/apps/backend/main.js": {
      "memory": 512,
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/apps/backend/main.js"
    },
    {
      "src": "/",
      "dest": "/apps/frontend"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/apps/frontend/assets/$1"
    }
  ]
}
