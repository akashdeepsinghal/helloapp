{

    "functions": {
        "api/main.js": {
        "memory": 512, 
        "maxDuration": 10
        }
    },

    "routes": [
        { "src": "/api/(.*)", "dest": "api/main.js" },
        {
        "source": "/",
        "destination": "/apps/frontend"
        },
        {
        "source": "/assets/(.*)",
        "destination": "/apps/frontend/assets/$1"
        }
    ]    

}

  